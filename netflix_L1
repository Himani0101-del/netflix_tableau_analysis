   "source": [
    "#splitting type into movie and tv show for more insights.\n",
    "data['is_tv_show'] = data['type'].apply(lambda x: 1 if x.lower() == 'tv show' else 0)\n",
    "\n",
    "\n",
    "#extracting seasons for tv shows\n",
    "# Create the 'seasons' column and initialize with NaN\n",
    "data['new_seasons'] = np.nan\n",
    "\n",
    "# Create a mask where duration contains \"Season\" or \"Seasons\" (case-insensitive)\n",
    "season_mask = data['duration'].str.contains('Season', case=False, na=False)\n",
    "\n",
    "# Apply extraction only to rows matching the season mask\n",
    "data.loc[season_mask, 'new_seasons'] = data.loc[season_mask, 'duration'].str.extract(r'(\\d+)')[0].astype(float)\n",
    "\n",
    "#extracting minutes for movies and converting it into numeric\n",
    "data['movie_duration'] = data['duration'].apply(\n",
    "    lambda x: int(x.split()[0]) if 'min' in x else None\n",
    ")\n",
    "\n",
    "data['movie_duration'] = pd.to_numeric(data['movie_duration'], errors='coerce')\n",
    "\n",
    "\n",
    "data.head(20)\n",
    "\n"
   ]
  },
