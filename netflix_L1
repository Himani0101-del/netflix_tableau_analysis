    "import pandas as pd\n",
    "\n",
    "# Loading the cleaned data again to make some adjustments in the country column\n",
    "data = pd.read_csv(\"cleaned_netflix_data.csv\")\n",
    "\n",
    "# Drop rows where 'country' is missing (optional)\n",
    "data = data.dropna(subset=['country'])\n",
    "\n",
    "# Split countries by comma and explode\n",
    "data['country'] = data['country'].str.split(', ')\n",
    "data = data.explode('country')\n",
    "\n",
    "# Remove extra spaces (just in case)\n",
    "data['country'] = data['country'].str.strip()\n",
    "\n",
    "# Save the new exploded dataset\n",
    "data.to_csv(\"netflix_cleaned_exploded.csv\", index=False)\n"
   ]
